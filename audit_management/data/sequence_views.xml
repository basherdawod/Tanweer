<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record id="audit_program_sequence" model="ir.sequence">
            <field name="name">audit.item Name</field>
            <field name="code">audit.item</field>
            <field name="prefix">AU/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="audit_report_line_sequence" model="ir.sequence">
            <field name="name">account.type.level</field>
            <field name="code">account.type.level</field>
            <field name="padding">2</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="audit_report_final_sequence" model="ir.sequence">
            <field name="name">audit.financial.program</field>
            <field name="code">audit.financial.program</field>
             <field name="prefix">RF/%(year)s/</field>
            <field name="padding">2</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="audit_account_account_sequence" model="ir.sequence">
            <field name="name">audit.account.account</field>
            <field name="code">audit.account.account</field>
             <field name="prefix">Char/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="financial_audit_customer_sequence" model="ir.sequence">
            <field name="name">financial.audit.customer</field>
            <field name="code">financial.audit.customer</field>
             <field name="prefix">REG/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>