<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record id="middel_east_sequence" model="ir.sequence">
            <field name="name">Middel East Name</field>
            <field name="code">middel.east</field>
            <field name="prefix">ME/%(year)s/%(month)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="middel_quotation_sequence" model="ir.sequence">
            <field name="name">Middel East Quotation</field>
            <field name="code">middel.quotation</field>
            <field name="prefix">QRF/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>