<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record id="e_card_code_sequence" model="ir.sequence">
            <field name="name">Card Code </field>
            <field name="code">petty.cash.card</field>
            <field name="prefix">1</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="e_request_code_sequence" model="ir.sequence">
            <field name="name">Request Code </field>
            <field name="code">petty.cash.request</field>
            <field name="prefix">RQF/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="e_request_submit_sequence" model="ir.sequence">
            <field name="name">Submit Sequence </field>
            <field name="code">petty.cash.submission</field>
            <field name="prefix">PCSAF/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_petty_cash_payment" model="ir.sequence">
            <field name="name">Petty Cash Payment</field>
            <field name="code">petty.cash.payment</field>
            <field name="prefix">PCP/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>

