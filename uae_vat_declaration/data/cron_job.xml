<odoo>
    <data noupdate="1">
        <record id="cron_check_vat_due_dates" model="ir.cron">
            <field name="name">Check VAT Due Dates</field>
            <field name="model_id" ref="model_vat_registration"/>
            <field name="state">code</field>
            <field name="code">model.check_vat_due_dates()</field>
            <field name="active">True</field>
            <field name="interval_number">1</field>  
            <field name="interval_type">days</field> 
            <field name="numbercall" eval="-1"/>
            <field name="doall" eval="False"/> 
        </record>
    </data>
</odoo>
