<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="group_vat_declaration_user" model="res.groups">
        <field name="name">VAT Declaration User</field>
        <field name="category_id" ref="base.module_category_accounting_accounting"/>
        <field name="implied_ids" eval="[(4, ref('account.group_account_user'))]"/>
    </record>

    <record id="group_vat_declaration_manager" model="res.groups">
        <field name="name">VAT Declaration Manager</field>
        <field name="category_id" ref="base.module_category_accounting_accounting"/>
        <field name="implied_ids" eval="[(4, ref('account.group_account_manager'))]"/>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
    </record>
</odoo>